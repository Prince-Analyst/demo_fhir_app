				<term_definitions code="at0000">
					<items id="text">SOFA score</items>
					<items id="description">A scoring system to grade and follow the development of organ dysfunction in six vital organ systems. Previously known as "Sepsis related Organ Failure Assessment".</items>
				</term_definitions>
				<term_definitions code="at0001">
					<items id="text">Event Series</items>
					<items id="description">@ internal @</items>
				</term_definitions>
				<term_definitions code="at0002">
					<items id="text">Any event</items>
					<items id="description">Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.</items>
				</term_definitions>
				<term_definitions code="at0003">
					<items id="text">Tree</items>
					<items id="description">@ internal @</items>
				</term_definitions>
				<term_definitions code="at0004">
					<items id="text">Respiration</items>
					<items id="description">The ratio between the partial pressure of oxygen (PaO₂) and the fraction of inspired oxygen (FiO₂) is an indicator for a possible dysfunction of the respiratory system.</items>
					<items id="comment">In some localities, the PaO₂/FiO₂ ratio is measured in kPa.</items>
				</term_definitions>
				<term_definitions code="at0005">
					<items id="text">PaO₂/FiO₂ ≥ 400 mmHg</items>
					<items id="description">The partial pressure of oxygen in arterial or capillary blood measured in mmHg, divided by the fraction of inspired oxygen is greater than or equal to 400 mmHg. Equivalent to PaO₂/FiO₂ ≥ 53.3 kPa.</items>
				</term_definitions>
				<term_definitions code="at0006">
					<items id="text">PaO₂/FiO₂ 300-399 mmHg</items>
					<items id="description">The partial pressure of oxygen in arterial or capillary blood measured in mmHg, divided by the fraction of inspired oxygen is between 300 and 399 mmHg. Equivalent to PaO₂/FiO₂ 40.0 - 53.2 kPa.</items>
				</term_definitions>
				<term_definitions code="at0007">
					<items id="text">PaO₂/FiO₂ &lt; 300 mmHg</items>
					<items id="description">The partial pressure of oxygen in arterial or capillary blood measured in mmHg, divided by the fraction of inspired oxygen is lower than 300 mmHg. Equivalent to PaO₂/FiO₂ 40 kPa.</items>
				</term_definitions>
				<term_definitions code="at0008">
					<items id="text">Mechanically ventilated and PaO₂/FiO₂ 100-199 mmHg</items>
					<items id="description">On mechanical ventilation, the partial pressure of oxygen in arterial or capillary blood measured in mmHg, divided by the fraction of inspired oxygen is between 100 and 199 mmHg. Equivalent to PaO₂/FiO₂ 13.3 - 26.5 kPa.</items>
				</term_definitions>
				<term_definitions code="at0009">
					<items id="text">Mechanically ventilated and PaO₂/FiO₂ &lt; 100 mmHg</items>
					<items id="description">On mechanical ventilation, the partial pressure of oxygen in arterial or capillary blood measured in mmHg, divided by the fraction of inspired oxygen is less than 100 mmHg. Equivalent to PaO₂/FiO₂ &lt; 13.3 kPa.</items>
				</term_definitions>
				<term_definitions code="at0010">
					<items id="text">Cardiovascular system</items>
					<items id="description">The mean arterial pressure (MAP), or the need for vasopressors (VP), (dopamine (DA), adrenaline (A), noradrenaline (NA) or dobutamine) are indicators for a possible dysfunction of the cardiovascular system.</items>
				</term_definitions>
				<term_definitions code="at0011">
					<items id="text">MAP ≥ 70 mmHg</items>
					<items id="description">The mean arterial pressure (MAP) is greater than or equal to 70 mmHg.</items>
				</term_definitions>
				<term_definitions code="at0012">
					<items id="text">MAP &lt; 70 mmHg</items>
					<items id="description">The mean arterial pressure (MAP) is less than 70 mmHg.</items>
				</term_definitions>
				<term_definitions code="at0013">
					<items id="text">DA ≤ 5; Dobutamine</items>
					<items id="description">The dopamine (DA) dosage is less than 5 µg/kg/minute, or any dosage of dobutamine.</items>
				</term_definitions>
				<term_definitions code="at0014">
					<items id="text">DA > 5; NA/A ≤ 0.1</items>
					<items id="description">The dosage of dopamine (DA) is greater than 5 µg/kg/minute, or the dosage of noradrenaline (NA) is less than or equal to 0.1 µg/kg/minute, or the dosage of adrenaline (A) is less than or equal to 0.1 µg/kg/minutt.</items>
				</term_definitions>
				<term_definitions code="at0015">
					<items id="text">NA/A > 0.1</items>
					<items id="description">The dosage of noradrenaline (N) or adrenaline (A) is greater than 0.1 µg/kg/minute.</items>
				</term_definitions>
				<term_definitions code="at0016">
					<items id="text">Central nervous system</items>
					<items id="description">The Glasgow Coma Scale (GCS) is an indicator for a possible dysfunction of the central nervous system.</items>
				</term_definitions>
				<term_definitions code="at0017">
					<items id="text">GCS 15</items>
					<items id="description">Glasgow Coma Scale is 15.</items>
				</term_definitions>
				<term_definitions code="at0018">
					<items id="text">GCS 13 - 14</items>
					<items id="description">Glasgow Coma Scale is 13 or 14.</items>
				</term_definitions>
				<term_definitions code="at0019">
					<items id="text">GCS 10 - 12</items>
					<items id="description">Glasgow Coma Scale is 10, 11 or 12.</items>
				</term_definitions>
				<term_definitions code="at0020">
					<items id="text">GCS 6 - 9</items>
					<items id="description">Glasgow Coma Scale is 6, 7, 8 or 9.</items>
				</term_definitions>
				<term_definitions code="at0021">
					<items id="text">GCS &lt; 6</items>
					<items id="description">Glasgow Coma Scale is less than 6.</items>
				</term_definitions>
				<term_definitions code="at0022">
					<items id="text">Renal function</items>
					<items id="description">Creatinine concentration and 24 h urine output (UOP) are indicators for a possible dysfunction of the central nervous system.</items>
					<items id="comment">In some localities, creatinine concentration is measured in μmol/L.</items>
				</term_definitions>
				<term_definitions code="at0023">
					<items id="text">Creatinine &lt; 1.2 mg/dL</items>
					<items id="description">The creatinine concentration is less than 1.2 mg/dL. Equivalent to creatinine &lt; 110 µmol/L.</items>
				</term_definitions>
				<term_definitions code="at0024">
					<items id="text">Creatinine 1.2-1.9 mg/dL</items>
					<items id="description">The creatinine concentration is between 1.2 and 1.9 mg/dL. Equivalent to creatinine 110 - 170 µmol/L.</items>
				</term_definitions>
				<term_definitions code="at0025">
					<items id="text">Creatinine 2.0-3.4 mg/dL</items>
					<items id="description">The creatinine concentration is between 2.0 and 3.4 mg/dL. Equivalent to creatinine 171 - 299 µmol/L.</items>
				</term_definitions>
				<term_definitions code="at0026">
					<items id="text">Creatinine 3.5-4.9 mg/dL or UOP &lt; 500 mL/24h</items>
					<items id="description">The creatinine concentration is between 3.5 and 4.9 mg/dL, or the urine output is between 500 and 200 mL/24h. Equivalent to creatinine 300 - 440 µmol/L or UOP &lt; 500 mL/24h.</items>
				</term_definitions>
				<term_definitions code="at0027">
					<items id="text">Creatinine ≥ 5.0 mg/dL or UOP &lt; 200 mL/24h</items>
					<items id="description">The creatinine concentration is greater than or equal to 5.0 mg/dL, or the urine output is less than 200 mL/24h. Equivalent to creatinine > 440 µmol/L or UOP &lt; 200 mL/24h.</items>
				</term_definitions>
				<term_definitions code="at0028">
					<items id="text">Liver function</items>
					<items id="description">Bilirubin concentration is an indicator for a possible dysfunction of the central nervous system.</items>
					<items id="comment">In some localities, bilirubin concentration is measured in μmol/L.</items>
				</term_definitions>
				<term_definitions code="at0029">
					<items id="text">Bilirubin &lt; 1.2 mg/dL</items>
					<items id="description">The bilirubin concentration is less than 1.2 mg/dL. Equivalent to bilirubin &lt; 20 µmol/L.</items>
				</term_definitions>
				<term_definitions code="at0030">
					<items id="text">Bilirubin 1.2-1.9 mg/dL</items>
					<items id="description">The bilirubin concentration is between 1.2 and 1.9 mg/dL. Equivalent to bilirubin 20-32 µmol/L.</items>
				</term_definitions>
				<term_definitions code="at0031">
					<items id="text">Bilirubin 2.0-5.9 mg/dL</items>
					<items id="description">The bilirubin concentration is between 2.0 and 5.9 mg/dL. Equivalent to bilirubin 33-101 µmol/L.</items>
				</term_definitions>
				<term_definitions code="at0032">
					<items id="text">Bilirubin 6.0-11.9 mg/dL</items>
					<items id="description">The bilirubin concentration is between 6.0 and 11.9 mg/dL. Equivalent to bilirubin 102-204 µmol/L.</items>
				</term_definitions>
				<term_definitions code="at0033">
					<items id="text">Bilirubin ≥ 12.0 mg/dL</items>
					<items id="description">The bilirubin concentration is greater than or equal to 12.0 mg/dL. Equivalent to bilirubin > 204 µmol/L.</items>
				</term_definitions>
				<term_definitions code="at0034">
					<items id="text">Blood clotting</items>
					<items id="description">Platelets concentration is an indicator for a possible dysfunction of the blood clotting system.</items>
				</term_definitions>
				<term_definitions code="at0035">
					<items id="text">Platelets ≥ 150 (x10³/µL)</items>
					<items id="description">The platelet concentration is greater than or equal to 150,000 /µL.</items>
				</term_definitions>
				<term_definitions code="at0036">
					<items id="text">Platelets &lt; 150 (x10³/µL)</items>
					<items id="description">The platelet concentration is between 150,000 and 100,000 /µL.</items>
				</term_definitions>
				<term_definitions code="at0037">
					<items id="text">Platelets &lt; 100 (x10³/µL)</items>
					<items id="description">The platelet concentration is between 100,000 and 50,000 /µL.</items>
				</term_definitions>
				<term_definitions code="at0038">
					<items id="text">Platelets &lt; 50 (x10³/µL)</items>
					<items id="description">The platelet concentration is between 50,000 and 20,000 /µL.</items>
				</term_definitions>
				<term_definitions code="at0039">
					<items id="text">Platelets &lt; 20 (x10³/µL)</items>
					<items id="description">The platelet concentration is less than 20,000 /µL.</items>
				</term_definitions>
				<term_definitions code="at0041">
					<items id="text">Total score</items>
					<items id="description">The total sum of each component parameter for the SOFA score.</items>
				</term_definitions>
				<term_definitions code="at0043">
