				<term_definitions code="at0000">
					<items id="text">Medication summary</items>
					<items id="description">Summary or persistent information about all use of a single medication by an individual.</items>
				</term_definitions>
				<term_definitions code="at0001">
					<items id="text">Tree</items>
					<items id="description">@ internal @</items>
				</term_definitions>
				<term_definitions code="at0002">
					<items id="text">Medication name</items>
					<items id="description">Name of medication used.</items>
					<items id="comment">Coding of  'Medication name' with a terminology capable of triggering decision support is strongly recommended where possible, using for example RxNorm, DM+D, Australian Medicines Terminology or FEST. Free text entry should only be used if there is no appropriate terminology available. For example: 'Zinacef 750 mg powder' or 'cefuroxim'. </items>
				</term_definitions>
				<term_definitions code="at0005">
					<items id="text">Tree</items>
					<items id="description">@ internal @</items>
				</term_definitions>
				<term_definitions code="at0006">
					<items id="text">Last updated</items>
					<items id="description">The date this medication summary was last updated.</items>
				</term_definitions>
				<term_definitions code="at0007">
					<items id="text">Clinical description</items>
					<items id="description">Narrative description about the use of the medication.</items>
				</term_definitions>
				<term_definitions code="at0008">
					<items id="text">Episode</items>
					<items id="description">Details about use of the medication during a specified period of time.</items>
				</term_definitions>
				<term_definitions code="at0009">
					<items id="text">Onset of use</items>
					<items id="description">The date when the medication was first used.</items>
					<items id="comment">Can be a partial date, for example, only a year.</items>
				</term_definitions>
				<term_definitions code="at0010">
					<items id="text">Cessation of use</items>
					<items id="description">The date when the medication was last used.</items>
					<items id="comment">Can be a partial date, for example, only a year.</items>
				</term_definitions>
				<term_definitions code="at0011">
					<items id="text">Episode onset</items>
					<items id="description">The date of onset for this episode.</items>
					<items id="comment">Can be a partial date, for example, only a year.</items>
				</term_definitions>
				<term_definitions code="at0012">
					<items id="text">Episode cessation</items>
					<items id="description">The date of cessation of use of this medication for this episode.</items>
					<items id="comment">Can be a partial date, for example, only a year.</items>
				</term_definitions>
				<term_definitions code="at0013">
					<items id="text">Reason for cessation</items>
					<items id="description">The reason why use of the medication was stopped.</items>
					<items id="comment">Coding with an external terminology is preferred, where possible. For example: sub-optimal control of diabetes; adverse reaction; or high cost.</items>
				</term_definitions>
				<term_definitions code="at0014">
					<items id="text">Description</items>
					<items id="description">Narrative description about the use of the medication during this episode.</items>
				</term_definitions>
				<term_definitions code="at0015">
					<items id="text">Cumulative dose</items>
					<items id="description">Total amount of the medication used over the lifetime of the individual.</items>
					<items id="comment">May be manually calculated or derived via the EHR from multiple sources.&#13;
&#13;
</items>
				</term_definitions>
				<term_definitions code="at0016">
					<items id="text">Episode amount</items>
					<items id="description">Total dose of the medication used in this episode.</items>
				</term_definitions>
				<term_definitions code="at0018">
					<items id="text">Episode indication</items>
					<items id="description">The clinical indication for the use of the medication during this episode, particularly if this is more specific or differs from the 'Clinical indication', or there is no clinical indication that applies for all episodes.</items>
					<items id="comment">Coding with an external terminology is preferred, where possible. For example: shingles; acute urine infection or chronic pyelonephritis.</items>
				</term_definitions>
				<term_definitions code="at0019">
					<items id="text">Extension</items>
					<items id="description">Additional information required to extend the model with local content or to align with other reference models or formalisms.</items>
					<items id="comment">For example: local information requirements; or additional metadata to align with FHIR.</items>
				</term_definitions>
				<term_definitions code="at0020">
					<items id="text">Therapeutic intent</items>
					<items id="description">The therapeutic intent for use of the medication during this episode.</items>
					<items id="comment">Coding with an external terminology, where possible. For example: pain relief; palliative or curative; short term course or life-long antibiotics.</items>
				</term_definitions>
				<term_definitions code="at0022">
					<items id="text">Treatment outcome</items>
					<items id="description">The observed response to the treatment with this medication during this episode.</items>
					<items id="comment">Coding with an external terminology is preferred, where possible. For example: UTI resolved</items>
				</term_definitions>
				<term_definitions code="at0027">
					<items id="text">Cumulative duration</items>
					<items id="description">The sum of the duration of all episodes.</items>
					<items id="comment">May be manually calculated or derived via the EHR from multiple sources.</items>
				</term_definitions>
				<term_definitions code="at0028">
					<items id="text">Clinical indication</items>
					<items id="description">The overall clinical indication for the use of the medication.</items>
					<items id="comment">Coding with an external terminology is preferred, where possible.</items>
				</term_definitions>
				<term_definitions code="at0029">
					<items id="text">Additional details</items>
					<items id="description">Additional details about medication use during this episode.</items>
				</term_definitions>
			</children>
